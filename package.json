    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "ioredis": "^4.6.2",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4"
  }
}
