{
  "name": "yoss_websocket",
  "version": "0.0.1",
  "description": "Yoss WebSockets main server",
  "main": "dist/index.js",
  "author": "Delwiv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "configure": "babel-node configure.js",
    "dev": "nodemon --ignore src/config.json --exec \"NODE_ENV=development yarn configure && babel-node src/index.js\"",
